"use strict";
(function($){
	$(document).ready(function(){
		//noinspection JSUnresolvedVariable
		var $invoices = $('<button type="submit" name="print" value="invoices">').addClass('alignleft').text(jigoshop_pdf_invoices.i18n.actions.print_invoices);
		//noinspection JSUnresolvedVariable
		var $packing_slips = $('<button type="submit" name="print" value="packing_slips">').addClass('alignleft').text(jigoshop_pdf_invoices.i18n.actions.print_packing_slips);
		$invoices.prependTo('div.tablenav');
		$packing_slips.prependTo('div.tablenav');
	});
})(jQuery);